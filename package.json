{
  "name": "flair-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "rimraf dist && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "docker compose -f docker-compose.dev.yml --env-file .env.development up -d && nest start",
    "start:dev": "docker compose -f docker-compose.dev.yml --env-file .env.development up -d && rimraf dist && nest start --watch",
    "start:debug": "docker compose -f docker-compose.dev.yml --env-file .env.development up -d && rimraf dist && nest start --debug --watch",
    "start:prod": "docker compose -f docker-compose.dev.yml --env-file .env.development up -d && rimraf dist && node -r tsconfig-paths/register dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,test}/**/*.ts\"",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
    "docker:test:up": "docker compose -f docker-compose.test.yml --env-file .env.test up -d --wait",
    "docker:test:down": "docker compose -f docker-compose.test.yml --env-file .env.test down --remove-orphans",
    "test:e2e": "npm run docker:test:down && npm run docker:test:up && rimraf dist && jest --config ./test/jest-e2e.json --verbose"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@google/generative-ai": "^0.13.0",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "argon2": "^0.40.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^7.1.1",
    "csv-parse": "^5.5.6",
    "express-session": "^1.18.0",
    "fast-geoip": "^1.1.88",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "http-terminator": "^3.2.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "ms": "^2.1.3",
    "nodemailer": "^6.10.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "ua-parser-js": "^2.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.3.9",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/axios": "^0.9.36",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.2",
    "@types/ms": "^0.7.34",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axios": "^1.8.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleNameMapper": {
      "^@modules/(.*)$": "<rootDir>/src/app/modules/$1",
      "^@core/(.*)$": "<rootDir>/src/app/core/$1",
      "^@config/(.*)$": "<rootDir>/src/app/config/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{!(module|repository.module),}.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
